{
    "rewrites": [{
            "source": "/api/:path*",
            "destination": "https://my-api.com/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "headers": [{
        "source": "/:path*",
        "headers": [{
            "key": "Cache-Control",
            "value": "s-maxage=1, stale-while-revalidate"
        }]
    }]
}